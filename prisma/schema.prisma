model NpAttendance {
  id         Int       @id @default(autoincrement())
  employeeId Int       @map("employee_id")
  empName    String?   @map("emp_name") @db.VarChar(255)
  date       DateTime  @db.Date
  inTime     DateTime? @map("in_time") @db.Time(0)
  outTime    DateTime? @map("out_time") @db.Time(0)
  loginHours DateTime? @map("login_hours") @db.Time(0)
  status     String?   @db.VarChar(255)
  createdAt  DateTime  @default(now()) @map("created_at") @db.Timestamp(0)
  updatedAt  DateTime  @default(now()) @updatedAt @map("updated_at") @db.Timestamp(0)
  totalHours DateTime? @map("total_hours") @db.Time(0)
  breakHours DateTime? @map("break_hours") @db.Time(0)
  clockTimes String?   @map("clock_times") @db.Text

  @@map("npattendance")
}

